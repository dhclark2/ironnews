.page-header
  = link_to stories_path, class: 'btn btn-default' do
    %span.glyphicon.glyphicon-list-alt
    Back
  = link_to edit_story_path(@story), class: 'btn btn-primary' do
    %span.glyphicon.glyphicon-pencil
    Edit
  %h1 Show story

%dl.dl-horizontal
  %dt #
  %dd= @story.serial_id
  %dt= Vote:
  %dd= ?
  %dt Title:
  %dd= @story.title
  %dt Link:
  %dd= @story.link
  %dt Timestamp of post:
  %dd= Time.now
  %dt Authors email:
  %dd= @story.authors_email
